{
    "compilerOptions": {
        // Target latest browsers
        "target": "ES2019",
        "lib": ["ES2019", "DOM", "DOM.Iterable"],

        "module": "ESNext",
        "moduleResolution": "Node",
        "incremental": true,
        "outDir": "./.ts-cache",
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "jsx":"react",
        "skipLibCheck": true
    },
    "include": ["./src/**/*"]
}